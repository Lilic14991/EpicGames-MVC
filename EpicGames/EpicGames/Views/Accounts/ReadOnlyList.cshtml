@model IEnumerable<EpicGames.Models.Account>

@{
    ViewBag.Title = "Accounts";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Accounts</h2>

<table id="accounts" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>Account</th>
            <th>Console type</th>
            
        </tr>
    </thead>
    <tbody></tbody>
</table>
@section scripts
{
    <script>
        $(document).ready(function () {
            var table = $("#accounts").DataTable({
                ajax: {
                    url: "/api/accounts",
                    dataSrc: ""
                },
                columns: [
                    {
                        data: "aName",
                    },
                    {
                        data: "consoleType.cName"
                    },
                ]
            });
        });
    </script>
}
                    
           
                    
                   
                    
               
            